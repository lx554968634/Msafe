<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@null"
    android:orientation="vertical" >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/rubbish_setting_tag_bg"
        android:gravity="center_vertical|left"
        android:paddingBottom="@dimen/padding_list"
        android:paddingLeft="@dimen/padding_list1"
        android:paddingTop="@dimen/padding_list"
        android:text="@string/setting_rubbish_str"
        android:textColor="@color/black"
        android:textSize="@dimen/rubbish_setting_tag_size" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/divider_height"
        android:background="@color/tab_textcolor_selected" />
    <!-- 扫描图片 -->

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_list" >

        <!-- 扫描说明 -->

        <LinearLayout
            android:id="@+id/rubbish_setting_tag0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginTop="@dimen/padding_list1"
            android:orientation="vertical"
            android:paddingBottom="@dimen/padding_list"
            android:paddingLeft="@dimen/padding_list2"
            android:paddingTop="@dimen/padding_list" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title0"
                android:textColor="@color/black"
                android:textSize="@dimen/rubbish_setting_title_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title1"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_des_size" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rubbish_switch_0"
            android:layout_width="45dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/padding_list2"
            android:background="@drawable/switch_gray"
            android:clickable="true" >

            <ImageView
                android:id="@+id/rubbish_switch_tag0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/rubbish_setting_divider_height"
                android:src="@drawable/switch_tag" />

            <ImageView
                android:id="@+id/rubbish_switch_tag1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/rubbish_setting_divider_height"
                android:src="@drawable/switch_tag"
                android:visibility="invisible" />
        </RelativeLayout>

        <!-- 扫描数据 -->

        <View
            android:id="@+id/grid_item_divi"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/divider_height_half"
            android:layout_below="@id/rubbish_setting_tag0"
            android:layout_marginLeft="@dimen/padding_list"
            android:layout_marginRight="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <!-- 扫描说明 -->

        <LinearLayout
            android:id="@+id/rubbish_setting_tag1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:orientation="vertical"
            android:paddingBottom="@dimen/padding_list"
            android:paddingLeft="@dimen/padding_list2"
            android:paddingTop="@dimen/padding_list" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title2"
                android:textColor="@color/black"
                android:textSize="@dimen/rubbish_setting_title_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title6"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_des_size" />
        </LinearLayout>

        <!-- 扫描数据 -->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/padding_list2"
            android:paddingBottom="@dimen/padding_list_helf"
            android:paddingTop="@dimen/padding_list_helf"
            android:text="@string/setting_rubbish_jiantou"
            android:textColor="@color/rubbish_setting_content_txtcolor"
            android:textSize="@dimen/padding_list4" />
    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_list"
        android:paddingLeft="@dimen/padding_list2"
        android:text="@string/setting_rubbish_title3"
        android:textColor="@color/rubbish_setting_content_txtcolor"
        android:textSize="@dimen/rubbish_setting_des_size" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/divider_height"
        android:layout_marginLeft="@dimen/padding_list"
        android:layout_marginRight="@dimen/padding_list"
        android:background="@color/gray_mainactivity_btn_press" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <!-- 扫描说明 -->

        <LinearLayout
            android:id="@+id/rubbish_setting_tag2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:orientation="vertical"
            android:paddingBottom="@dimen/padding_list"
            android:paddingLeft="@dimen/padding_list2"
            android:paddingTop="@dimen/padding_list" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title3"
                android:textColor="@color/black"
                android:textSize="@dimen/rubbish_setting_title_size" />

        </LinearLayout>

        <!-- 扫描数据 -->

        <RelativeLayout
            android:id="@+id/rubbish_switch_1"
            android:layout_width="45dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/padding_list2"
            android:background="@drawable/switch_gray"
            android:clickable="true" >

            <ImageView
                android:id="@+id/rubbish_switch_tag00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/rubbish_setting_divider_height"
                android:src="@drawable/switch_tag" />

            <ImageView
                android:id="@+id/rubbish_switch_tag01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/rubbish_setting_divider_height"
                android:src="@drawable/switch_tag"
                android:visibility="invisible" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/divider_height_half"
            android:layout_below="@id/rubbish_setting_tag2"
            android:layout_marginLeft="@dimen/padding_list"
            android:layout_marginRight="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <!-- 扫描说明 -->

        <LinearLayout
            android:id="@+id/rubbish_setting_tag3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:orientation="vertical"
            android:paddingBottom="@dimen/padding_list"
            android:paddingLeft="@dimen/padding_list2"
            android:paddingTop="@dimen/padding_list" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title4"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_title_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title7"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_des_size" />
        </LinearLayout>

        <!-- 扫描数据 -->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/padding_list2"
            android:padding="@dimen/padding_list_helf"
            android:background="@drawable/tribe" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/divider_height_half"
            android:layout_below="@id/rubbish_setting_tag3"
            android:layout_marginLeft="@dimen/padding_list"
            android:layout_marginRight="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <!-- 扫描说明 -->

        <LinearLayout
            android:id="@+id/rubbish_setting_tag4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:orientation="vertical"
            android:paddingBottom="@dimen/padding_list"
            android:paddingLeft="@dimen/padding_list2"
            android:paddingTop="@dimen/padding_list" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title5"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_title_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_rubbish_title8"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/rubbish_setting_des_size" />
        </LinearLayout>

        <!-- 扫描数据 -->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/padding_list2"
            android:padding="@dimen/padding_list_helf"
            android:background="@drawable/tribe" />
    </RelativeLayout>

</LinearLayout>