<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pop_dialog_fileadmin_types"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_0"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/file_type0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_0"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype0"
                android:textColor="@color/fileadmin_pop_filetype_color_enable"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>

        <View
            android:layout_width="@dimen/divider_height"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_1"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/file_tpye1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_1"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype1"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>

        <View
            android:layout_width="@dimen/divider_height"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_2"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/file_type2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_2"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype2"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>
    </LinearLayout>

    <View
        android:layout_width="wrap_content"
        android:layout_height="@dimen/divider_height"
        android:layout_marginLeft="@dimen/padding_list"
        android:layout_marginRight="@dimen/padding_list"
        android:background="@color/gray_mainactivity_btn_press" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_3"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/file_type3" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_3"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype3"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>

        <View
            android:layout_width="@dimen/divider_height"
            android:layout_height="fill_parent"
            android:layout_marginBottom="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_4"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/file_type4" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_4"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype4"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>

        <View
            android:layout_width="@dimen/divider_height"
            android:layout_height="fill_parent"
            android:layout_marginBottom="@dimen/padding_list"
            android:background="@color/gray_mainactivity_btn_press" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_list_helf"
            android:layout_marginTop="@dimen/padding_list_helf"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/fileadmin_pop_filetype_5"
                android:layout_width="@dimen/all_item_app_icon_size"
                android:layout_height="@dimen/all_item_app_icon_size"
                android:layout_centerHorizontal="true"
                android:padding="@dimen/padding_list_helf"
                android:src="@drawable/fileadmin_item_default" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/fileadmin_pop_filetype_5"
                android:layout_centerHorizontal="true"
                android:text="@string/fileadmin_pop_filetype5"
                android:textColor="@color/rubbish_setting_content_txtcolor"
                android:textSize="@dimen/main_scan_content_size" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>